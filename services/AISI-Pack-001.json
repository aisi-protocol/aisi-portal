{
  "AISI-Protocol-Version": "1.0",
  "service-id": "AISI-Pack-001",
  "service-name": "智能旅行打包原子服务",
  "service-description": "基于目的地、季节、旅行天数和活动类型，生成个性化的旅行打包清单。这是AISI协议的原子服务样板，展示AI友好的确定性服务设计。",
  "service-provider": "AISI Protocol Core Team",
  "service-version": "1.0.0",
  
  "ai-optimized": true,
  "ai-benefits": [
    "明确的输入输出格式",
    "无歧义的结果生成",
    "支持AI自主调用和组合",
    "提供完整的上下文信息"
  ],
  
  "service-function": "生成旅行打包清单",
  
  "input-parameters": {
    "destination": {
      "type": "string",
      "required": true,
      "description": "旅行目的地（城市/国家）",
      "examples": ["深圳", "东京", "巴黎", "纽约"]
    },
    "season": {
      "type": "string", 
      "required": true,
      "enum": ["春", "夏", "秋", "冬"],
      "description": "旅行季节"
    },
    "duration-days": {
      "type": "integer",
      "required": true,
      "min": 1,
      "max": 30,
      "description": "旅行天数"
    },
    "activity-types": {
      "type": "array",
      "required": true,
      "items": {
        "type": "string",
        "enum": ["商务", "休闲", "探险", "文化", "海滩", "滑雪", "城市观光"]
      },
      "description": "主要活动类型"
    },
    "special-needs": {
      "type": "array",
      "required": false,
      "items": {
        "type": "string"
      },
      "description": "特殊需求（如：过敏药物、特殊设备等）"
    }
  },
  
  "output-format": {
    "packing-lists": {
      "categories": ["服装", "洗漱用品", "电子设备", "文档", "其他"],
      "items": {
        "name": "string",
        "quantity": "string",
        "importance": "high|medium|low",
        "notes": "string"
      }
    },
    "weather-advice": "string",
    "local-tips": "string",
    "checklist-summary": {
      "total-items": "integer",
      "must-have-items": "integer",
      "optional-items": "integer"
    }
  },
  
  "deterministic-examples": [
    {
