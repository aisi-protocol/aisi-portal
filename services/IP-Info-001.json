{
  "name": "IP-Info-001",
  "version": "1.0",
  "description": "IP地址地理信息查询服务",
  "provider": "AISI",
  "namespace": "AISI",
  "category": "network",
  "status": "active",
  "endpoint": "internal://ip-service",
  "parameters": {
    "required": ["ip"],
    "optional": ["lang"],
    "schema": {
      "ip": {
        "type": "string",
        "description": "IPv4或IPv6地址",
        "format": "ip-address",
        "example": "8.8.8.8"
      },
      "lang": {
        "type": "string",
        "description": "返回语言，zh-CN或en",
        "default": "zh-CN",
        "enum": ["zh-CN", "en"]
      }
    }
  },
  "response_format": {
    "success": {
      "ip": "string",
      "country": "string",
      "region": "string",
      "city": "string",
      "isp": "string",
      "coordinates": {
        "latitude": "number",
        "longitude": "number"
      }
    },
    "error": {
      "code": "string",
      "message": "string"
    }
  },
  "examples": [
    {
      "aisi_uri": "aisi://AISI/IP-Info-001?ip=8.8.8.8",
      "description": "查询Google DNS服务器位置",
      "expected_response": {
        "ip": "8.8.8.8",
        "country": "美国",
        "region": "加利福尼亚",
        "city": "山景城",
        "isp": "Google",
        "coordinates": {
          "latitude": 37.4056,
          "longitude": -122.0775
        }
      }
    },
    {
      "aisi_uri": "aisi://AISI/IP-Info-001?ip=1.1.1.1&lang=en",
      "description": "查询Cloudflare DNS位置（英文）"
    }
  ],
  "tags": ["ip", "geolocation", "network"],
  "created_at": "2026-01-20",
  "updated_at": "2026-01-21"
}
