{
  "name": "Holiday-CN-001",
  "version": "1.0",
  "description": "中国节假日查询服务",
  "provider": "AISI",
  "namespace": "AISI",
  "category": "calendar",
  "status": "active",
  "endpoint": "internal://holiday-service",
  "parameters": {
    "required": ["date"],
    "optional": ["detail"],
    "schema": {
      "date": {
        "type": "string",
        "description": "查询日期，格式：YYYY-MM-DD",
        "format": "date",
        "example": "2026-02-17"
      },
      "detail": {
        "type": "boolean",
        "description": "是否返回详细信息",
        "default": false
      }
    }
  },
  "response_format": {
    "success": {
      "date": "string",
      "is_holiday": "boolean",
      "name": "string",
      "days_left": "number",
      "is_workday": "boolean",
      "detail": {
        "description": "string",
        "duration": "number",
        "traditional_name": "string"
      }
    },
    "error": {
      "code": "string",
      "message": "string"
    }
  },
  "examples": [
    {
      "aisi_uri": "aisi://AISI/Holiday-CN-001?date=2026-02-17",
      "description": "查询2026年春节是否为节假日",
      "expected_response": {
        "date": "2026-02-17",
        "is_holiday": true,
        "name": "春节",
        "days_left": 27,
        "is_workday": false
      }
    },
    {
      "aisi_uri": "aisi://AISI/Holiday-CN-001?date=2026-01-22&detail=true",
      "description": "查询今日节假日详情"
    }
  ],
  "tags": ["holiday", "china", "calendar", "festival"],
  "created_at": "2026-01-20",
  "updated_at": "2026-01-21"
}
